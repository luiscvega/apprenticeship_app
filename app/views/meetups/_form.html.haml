= form_for [current_apprenticeship, @meetup] do |f|
  .field
    = f.label :venue
    = f.text_field :venue
  .field
    = f.label :date
    = f.date_select :date
  .field
    = f.label :time
    = f.time_select :time
  .field
    = f.label :description
    %br/
    = f.text_area :description, :rows => 2
  .action
    = f.submit