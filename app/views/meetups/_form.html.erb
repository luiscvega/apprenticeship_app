<%= form_for @meetup, :url => apprenticeship_meetup_url do |f|%>
  <div class="field">
    <%= f.label :venue %>
    <%= f.text_field :venue %>
  </div>
  <div class="field">
    <%= f.label :date %>
    <%= f.date_select :date %>
  </div>
  <div class="field">
    <%= f.label :time %>
    <%= f.time_select :time %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description, :rows => 2 %>
  </div>
  <div class="action">
    <%= f.submit "Send" %>
  </div>
<% end %>